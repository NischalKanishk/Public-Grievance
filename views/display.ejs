    <!-- Preloader Start -->
    
    <!-- Preloader Start -->
    
    <main>

       
        <!-- Job List Area Start -->
        <div class="job-listing-area pt-120 pb-120">
            <div class="container">
                <div class="row">
                    
                    <!-- Right content -->
                    <div class="col-xl-9 col-lg-9 col-md-8">
                        <!-- Featured_job_start -->
                        <section class="featured-job-area">
                            <div class="container">
                              <% for(var i=0; i < info.length; i++) { %>
                                <!-- single-job-content -->
                                <div class="single-job-items mb-30">
                                    <div class="job-items">
                                        
                                        <div class="job-tittle job-tittle2">
                                            <a href=>
                                                <h4><%= info[i].FIRSTNAME %> <%= info[i].LASTNAME %></h4>
                                            </a>
                                            <ul>
                                                <li><%= info[i].ID %></li>
                                                <li><i class="fas fa-flag"></i><%= info[i].DEPARTMENT %></li>
                                                <li id="status"></li>
                                            </ul>
                                            <p><%= info[i].REPORT %></p>
                                        </div>
                                    </div>
                                    <div class="ui buttons">
                                        <button style="height: 40px;" class="ui green basic button">Accept</button>
                                          <button style="height: 40px;" class="ui orange basic button">Resolve</button>
                                          <button style="height: 40px;" class="ui red basic button">Reject</button>
                                    </div>
                                    
                                <!-- single-job-content -->
                               
                                
                            </div>
                            <% } %>
                        </section>
                        <!-- Featured_job_end -->
                    </div>
                </div>
            </div>
        </div>
        
        
    </main>


